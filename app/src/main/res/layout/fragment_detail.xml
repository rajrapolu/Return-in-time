<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/tool_bar"
        layout="@layout/toolbar" />

    <TextView
        android:id="@+id/detail_text_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tool_bar"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Title"/>

    <TextView
        android:id="@+id/detail_text_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_text_title"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Caption"/>

    <TextView
        android:id="@+id/detail_text_return_to_key"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="Return To: "
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_text_type"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2"/>

    <TextView
        android:id="@+id/detail_text_return_to_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/detail_text_return_to_key"
        app:layout_constraintLeft_toRightOf="@+id/detail_text_return_to_key"
        app:layout_constraintTop_toTopOf="@+id/detail_text_return_to_key"
        app:layout_constraintVertical_bias="1.0"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body1" />

    <TextView
        android:id="@+id/detail_text_borrowed_key"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="Borrowed On: "
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_text_return_to_key"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/detail_text_borrowed_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/detail_text_borrowed_key"
        app:layout_constraintLeft_toRightOf="@+id/detail_text_borrowed_key"
        app:layout_constraintTop_toTopOf="@+id/detail_text_borrowed_key"
        app:layout_constraintVertical_bias="1.0"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body1" />

    <TextView
        android:id="@+id/detail_text_return_key"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="Return By: "
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_text_borrowed_key"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/detail_text_return_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/detail_text_return_key"
        app:layout_constraintLeft_toRightOf="@+id/detail_text_return_key"
        app:layout_constraintTop_toTopOf="@+id/detail_text_return_key"
        app:layout_constraintVertical_bias="1.0"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body1" />

    <TextView
        android:id="@+id/detail_text_notify_key"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notify Me On: "
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/detail_text_return_key"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body2" />

    <TextView
        android:id="@+id/detail_text_notify_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/detail_text_notify_key"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintTop_toTopOf="@+id/detail_text_notify_key"
        app:layout_constraintLeft_toRightOf="@+id/detail_text_notify_key"
        android:layout_marginLeft="0dp"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body1" />


</android.support.constraint.ConstraintLayout>

